{
  "version": "0.1.0",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "registry": "https://registry.npmjs.org/",
  "command": {
    "version": {
      "allowBranch": "master"
    },
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): Publish",
      "ignoreChanges": [
        "*.md",
        "**/__tests__/**",
        "**/__mocks__/**",
        "**/__testfixtures__/**"
      ]
    }
  }
}
