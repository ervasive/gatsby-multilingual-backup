{
  "version": "0.1.1-alpha.0",
  "packages": ["packages/*"],
  "command": {
    "version": {
      "allowBranch": "master"
    },
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): Publish",
      "ignoreChanges": [
        "*.md",
        "**/__tests__/**",
        "**/__mocks__/**",
        "**/__testfixtures__/**"
      ]
    }
  }
}
