{
  "version": "0.1.1-alpha.0",
  "packages": [
    "packages/*",
    "e2e-tests/generic-tests/*",
    "e2e-tests/invalid-translation-file/*"
  ],
  "command": {
    "version": {
      "allowBranch": "master"
    },
    "publish": {
      "allowBranch": "master",
      "message": "chore(release): Publish",
      "ignoreChanges": [
        "e2e-tests/**",
        "*.md",
        "**/__tests__/**",
        "**/__mocks__/**",
        "**/__testfixtures__/**"
      ]
    }
  }
}
